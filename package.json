{
  "name": "webcam-n8n-connector",
  "version": "1.0.0",
  "description": "Website to capture webcam photos and send to n8n workflow",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm install && ts-node src/server.ts",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "prod": "npm run build && node dist/server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0"
  }
}
